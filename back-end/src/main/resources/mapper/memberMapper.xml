<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.backend.auth.MemberMapper">

    <select id="loadUserByUserName" parameterType="com.example.backend.auth.model.Member" resultType="com.example.backend.auth.model.Member">
        SELECT * FROM member
        WHERE member_id = #{member_id}
    </select>
</mapper>
