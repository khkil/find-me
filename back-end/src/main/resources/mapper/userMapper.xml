<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.backend.mapper.UserMapper">

    <insert id="insertUser" parameterType="com.example.backend.model.User">
        INSERT INTO user(user_name, user_age, user_gender) VALUES (#{user_name}, #{user_age}, #{user_gender});
        <selectKey resultType="int" keyProperty="user_idx" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
    </insert>


</mapper>
