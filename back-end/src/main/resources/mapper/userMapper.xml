<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.backend.mapper.UserMapper">

    <select id="getUserCount" parameterType="com.example.backend.model.Inspection" resultType="int">
        SELECT COUNT(*) FROM user WHERE inspection_idx = #{inspection_idx};
    </select>


</mapper>
